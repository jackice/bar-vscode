{
    "name": "bar",
    "displayName": "Build and run",
    "description": "Click a button to compile/build and/or run a project",
    "version": "0.0.1",
    "publisher": "olback",
    "icon": "images/bar_icon.png",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.init",
        "onCommand:extension.build",
        "onCommand:extension.run",
        "onCommand:extension.bar"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.init",
            "title": "Bar: Init"
        },
        {
            "command": "extension.run",
            "title": "Bar: Run project"
        },
        {
            "command": "extension.build",
            "title": "Bar: Build project"
        },
        {
            "command": "extension.bar",
            "title": "Bar: Build and run project"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}